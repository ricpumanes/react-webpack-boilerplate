{
  "name": "webpack-react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "scripts": {
    "build": "webpack --env.NODE_ENV=prod --config ./webpack/webpack.config-prod.js --mode production",
    "clean": "rm -rf ./build",
    "dev": "webpack-dev-server --env.NODE_ENV=local --mode development --config ./webpack/webpack.config.js",
    "prod": "npm run clean; npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint '*/**/*.js'",
    "lint-fix": "eslint '*/**/*.js' --fix",
    "simulate": "node ./servers/dev.js"
  },
  "keywords": [
    "react",
    "redux",
    "boilerplate",
    "webpack",
    "tailwind",
    "styled-components",
    "feathers"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@sheerun/mutationobserver-shim": "^0.3.3",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.4.1",
    "@testing-library/user-event": "^12.0.7",
    "autoprefixer": "^9.8.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "css-loader": "^3.6.0",
    "css-mqpacker": "7.0.0",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^26.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "3.0.0",
    "prettier": "2.0.5",
    "react-hot-loader": "4.11.1",
    "redux-logger": "3.0.6",
    "redux-mock-store": "1.5.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.6",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "@feathersjs/authentication-client": "^4.5.4",
    "@feathersjs/express": "^4.5.4",
    "@feathersjs/feathers": "^4.5.3",
    "@feathersjs/socketio-client": "^4.5.4",
    "prop-types": "15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "2.3.0",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.1.1",
    "tailwindcss": "^1.4.6",
    "validate.js": "0.13.1"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setupTests.js",
      "<rootDir>/__tests__/utils/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/__tests__/setupTests.js"
    ],
    "moduleNameMapper": {
      "^RootDir(.*)$": "<rootDir>/src$1"
    }
  }
}
